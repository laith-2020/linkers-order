    <div id="myModal" class="modal">

        <div class="modal-content">
            <div class="modal-header">

                <i id="leftArrow" style='font-size:24px' class='fas'>&#xf104;</i>

                <span class="close">&times;</span>

            </div>
            <div class="modal-body">

                <img src="<%=resultKey.coverImage %>" style="width:100%; height:200px">
                <br>
                <h1 class="shopName">
                    <%=resultKey.shopName %>
                </h1>
                <p class="cityName">
                    <i class="fas fa-map-marker-alt"></i>
                    <%=resultKey.city %>
                        <%=resultKey.district %>
                </p>

                <p class="deliveryFees">
                    <%=resultKey.deliveryFees%>
                </p>
                <p class="deliveryTime">
                    <i class="far fa-clock"></i> Delivery Time :
                    <%=resultKey.deliveryTime %> Min <i class="fas fa-shopping-bag"></i>
                        <%=resultKey.minimumOrder %> RS , Minimum Order
                </p>
                <div class="stickeNav">

                    <nav class="navAnk">
                        <%categoryKey.map((key,idx)=> {%>

                            <a href="#<%=key%>" id="ankerCateg" class="ank">
                                <%=key%>
                            </a>

                            <%})%>

                    </nav>
                </div>

                <hr>


                <%categoryKey.map((key,idx)=> {%>
                    <li id="<%=key%>" class="categName">
                        <%=key%>
                            <%productKey.map((item,idx) => {%>
                                <div id="<%=item.productName.replace(/\s/g, '')%>" class="theProduct productC">
                                    <% if(item.category === key){ %>


                                        <img id="<%=item.productName.replace(/\s/g, '')%>" class="productImg" src="<%=item.productImages%>" style="width: 100px; height: 100px; border-radius: 10%; ">


                                        <p id="<%=item.productName.replace(/\s/g, '')%>" class="productName">
                                            <%=item.productName%>
                                        </p>
                                        <p id="<%=item.productName.replace(/\s/g, '')%>" class="productDesc">
                                            <%=item.description%>
                                        </p>
                                        <p id="<%=item.productName.replace(/\s/g, '')%>" class="productC " style="color: black; padding-left:27px;">

                                            <span class="productPrice"><%=item.price%></span>
                                        </p>

                                        <p class="groupName">
                                            <%=item.groupName%>
                                        </p>


                                        <form class="featureName2">
                                            <%if(item.featureName[0] !== ''){%>
                                                <%item.featureName.forEach ((itm,idx) =>{%>

                                                    <input type="checkbox" id="featureName" class="<%=item.extraPrice[idx]%>" value="<%=itm%>" name="featureName">

                                                    <%=itm%>

                                                        <div id="thePrice">
                                                            <span class="productC"> <%=item.extraPrice[idx]%> </span>
                                                        </div>
                                                        <br>
                                                        <br>

                                                        <%})%>
                                                            <%}%>

                                        </form>

                                </div>
                                <hr>
                                <%}%>
                                    <%})%>
                                        <% })%>
                    </li>

            </div>
            <div class="modal-footer">

                <section id="addToCartSection">

                    <button id="addToCartBtn" class="cartBtn productC">
                        <span>Add To Cart</span>
                        <svg width="13px" height="10px" viewBox="0 0 13 10">
                            <path d="M1,5 L11,5"></path>
